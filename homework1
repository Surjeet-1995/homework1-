/* this code doesn't contain any pthread files or function */ 
 
 #include<stdio.h>
#include<stdlib.h>

int my_array[] { 1, 2, 3, 4, 5 };
size_t L = sizeof(my_array)/sizeof(int);

int compar (void * pa, void * pb)
{
int a = *((int*)pa);
int b = *((int*)pb);
int ret =0;
if(a<b)
ret=-1;
else if (a>b)
ret=1;
else
return ret;
}

int main ()
{
int i;
int j;
printf("Before ");
for (j=0; j<L; j++)
printf ("%d ",my_array[j]);
printf("\nAfter ");
qsort (my_array, L, sizeof(int), compar );
for (i=0; i<L; i++)
printf ("%d ",my_array[i]);
return 0;

}
